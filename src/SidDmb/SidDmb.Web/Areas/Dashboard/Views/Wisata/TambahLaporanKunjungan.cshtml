    <!-- end page title -->

    <div class="row">
        <div class="col-12">
            <div class="card">
                <div class="card-body">
                    <h4 class="header-title">Tambah Laporan Kunjungan</h4>
                    <div class="row">
                        <div class="col-lg-6">
                            <form>

                                <div class="mb-3">
                                    <label for="simpleinput" class="form-label">ID<span class="text-danger">*</span></label>
                                    <input type="text" id="simpleinput" class="form-control" autofocus>
                                </div>

                                <div class="mb-3">
                                    <label for="example-time" class="form-label">Tanggal Kunjungan<span class="text-danger">*</span></label>
                                    <input class="form-control" id="example-time" type="date" name="time">
                                </div>

                                <div class="mb-3">
                                    <label for="simpleinput" class="form-label">Nama<span class="text-danger">*</span></label>
                                    <input type="text" id="simpleinput" class="form-control">
                                </div>

                                <div class="mb-3">
                                    <label for="simpleinput" class="form-label">Jumlah Wisatawan Domestik<span class="text-danger">*</span></label>
                                    <input type="number" id="simpleinput" class="form-control">
                                </div>

                                <div class="mb-3">
                                    <label for="simpleinput" class="form-label">Jumlah Wisatawan Internasional<span class="text-danger">*</span></label>
                                    <input type="number" id="simpleinput" class="form-control">
                                </div>

                                <div class="mb-3">
                                    <label for="simpleinput" class="form-label">Durasi Kunjungan<span class="text-danger">*</span></label>
                                    <input type="number" id="simpleinput" class="form-control">
                                </div>                              

