@{
    ViewData["Title"] = "Home";
}

<div class="container-fluid">

    <!-- start page title -->
    <div class="row">
        <div class="col-12">
            <div class="page-title-box">
                <div class="page-title-right">
                    <ol class="breadcrumb m-0">
                        <li class="breadcrumb-item"><a href="javascript: void(0);">Dashboard</a></li>
                        <li class="breadcrumb-item active">Dashboard</li>
                    </ol>
                </div>
                <h4 class="page-title">Dashboard</h4>
            </div>
        </div>
    </div>
    <!-- end page title -->

</div>
   
<div class="row">
    <div class="col-12">

        <div class="row g-0 mb-1">
            <div class="col-sm-6 col-xl-3">
                <div class="card shadow-none m-0">
                    <div class="card-body text-center">
                        <i class="uil-scenery" style="font-size: 24px;"></i>
                        <h3><span>29</span></h3>
                        <p class="text-muted font-15 mb-0">Total Destinasi Wisata</p>
                    </div>
                </div>
            </div>

            <div class="col-sm-6 col-xl-3">
                <div class="card shadow-none m-0 border-start">
                    <div class="card-body text-center">
                        <i class="uil-archway" style="font-size: 24px;"></i>
                        <h3><span>715</span></h3>
                        <p class="text-muted font-15 mb-0">Total Budaya</p>
                    </div>
                </div>
            </div>

            <div class="col-sm-6 col-xl-3">
                <div class="card shadow-none m-0 border-start">
                    <div class="card-body text-center">
                        <i class="uil-briefcase" style="font-size: 24px;"></i>
                        <h3><span>31</span></h3>
                        <p class="text-muted font-15 mb-0">Total Preneur</p>
                    </div>
                </div>
            </div>

            <div class="col-sm-6 col-xl-3">
                <div class="card shadow-none m-0 border-start">
                    <div class="card-body text-center">
                        <i class="uil-star" style="font-size: 24px;"></i>
                        <h3><span>93</span></h3>
                        <p class="text-muted font-15 mb-0">Prima</p>
                    </div>
                </div>
            </div>

        </div> <!-- end row -->

        <div class="row g-0 mb-1">
            <div class="col-sm-6 col-xl-3">
                <div class="card shadow-none m-0">
                    <div class="card-body text-center">
                        <i class="uil-calendar-alt" style="font-size: 24px;"></i>
                        <h3><span>29</span></h3>
                        <p class="text-muted font-15 mb-0">Total Event</p>
                    </div>
                </div>
            </div>

            <div class="col-sm-6 col-xl-3">
                <div class="card shadow-none m-0 border-start">
                    <div class="card-body text-center">
                        <i class="uil-books" style="font-size: 24px;"></i>
                        <h3><span>715</span></h3>
                        <p class="text-muted font-15 mb-0">Pelatihan & Edukasi</p>
                    </div>
                </div>
            </div>

            <div class="col-sm-6 col-xl-3">
                <div class="card shadow-none m-0 border-start">
                    <div class="card-body text-center">
                        <i class="uil-box" style="font-size: 24px;"></i>
                        <h3><span>31</span></h3>
                        <p class="text-muted font-15 mb-0">Total Produk</p>
                    </div>
                </div>
            </div>

            <div class="col-sm-6 col-xl-3">
                <div class="card shadow-none m-0 border-start">
                    <div class="card-body text-center">
                        <i class="uil-search-alt" style="font-size: 24px;"></i>
                        <h3><span>87</span></h3>
                        <p class="text-muted font-15 mb-0">Development</p>
                    </div>
                </div>
            </div>

        </div> <!-- end row -->        

        <div class="card widget-inline">



            <div class="card-body p-0">
                
            </div>
        </div> <!-- end card-box-->
    </div> <!-- end col-->
</div>
<!-- end row-->
